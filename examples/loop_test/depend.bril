main {
  a: int = const 0;
  b: int = const 1;
  i: int = const 10;
  zero: int = const 0;

header:
  j: int = const 7;
  cond: bool = gt i zero;
  br cond bo exit;

bo:
  ded: int = const 3; #unused
  one: int = const 1;
  dedo: int = mul ded one;
  j: int = sub j one;
  cond: bool = gt j zero;
  br cond body header;

body:
  print a;
  a: int = add b zero;
  b: int = add a b;
  i: int = sub i one;
  jmp bo;

exit:
  nop;

}
